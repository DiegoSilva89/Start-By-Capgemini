algoritmo "Aula 2.J - Escolha"

var

voto: literal

inicio

Escreval("Digite seu voto")
leia(voto)

Escolha voto
caso "20"
     Escreval("Você votou em Marcelo Bernart")
caso "30"
     Escreval("Você votou no Pernalonga")
caso "40"
     Escreval("Você votou no Patoline")
outrocaso
     Escreval("Voto nulo")
fimescolha

fimalgoritmo
